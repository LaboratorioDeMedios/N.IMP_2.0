<MAIN_SETTINGS>
    <SETTINGS>
        <INPUTS>
            <!-- black BG -->
            <INPUT name="Black" type="IMAGE" path="sets/black.png"/>
            <!-- FARC -->
            <INPUT name="FARC" type="IMAGE" palindrom="true" multiplier_divider="32" bpm="100" >
                <ASSET name="farc" path="sets/farc/INSUMOS NOCHE DE FALLOS.mov"/>
            </INPUT>
            <!-- city views modified and unmodified -->
            <INPUT name="City" type="IMAGE" palindrom="true" multiplier_divider="32" bpm="120" >
                <ASSET name="contorno" path="sets/city/contorno ciudad.mov"/>
                <ASSET name="frontal edificios" path="sets/city/frontal edificios.mov"/>
                <ASSET name="shadow" path="sets/city/sun-shadow.mov"/>
                <ASSET name="techos ciudad" path="sets/city/techos ciudad.mov"/>
            </INPUT>
            <!--img sequences -->
            <INPUT name="imgSequences" type="IMAGE" palindrom="true" multiplier_divider="2" bpm="100" >
                <ASSET name="contorno" path="sets/imgSequences/satellite"/>
                <ASSET name="frontal edificios" path="sets/imgSequences/waves/waves"/>
                <ASSET name="frontal edificios" path="sets/imgSequences/waves/waves2"/>
                <ASSET name="frontal edificios" path="sets/imgSequences/waves/waves3"/>
            </INPUT>
            <!-- nature stuff -->
            <INPUT name="Nature" type="IMAGE" palindrom="true" multiplier_divider="32" bpm="100" >
                <ASSET name="clouds" path="sets/nature/clouds.mov"/>
                <ASSET name="baloon" path="sets/nature/ballon.mov"/>
                <ASSET name="hojas" path="sets/nature/hojas_cielo lento .mov"/>
                <ASSET name="mar" path="sets/nature/mar .mov"/>
                <ASSET name="stars" path="sets/nature/stars/0.mov"/>
                <ASSET name="stars2" path="sets/nature/stars/1.mov"/>
                <ASSET name="moon dawn" path="sets/nature/moon_dawn.mov"/>
                <ASSET name="recorrido_parque_" path="sets/nature/recorrido_parque_.mov"/>
                <ASSET name="aquas gotas" path="sets/nature/agua_gotas .mov"/>
                <ASSET name="aquas gotas2" path="sets/nature/agua_gotas_ .mov"/>
            </INPUT>
            <!-- textures -->
            <!-- Fast -->
            <INPUT name="Fast-Red" type="IMAGE" bpm="100">
                <ASSET name="edificios rojos  " path="sets/textures/fast/red/edificios rojos  .mov"/>
                <ASSET name="textura roja" path="sets/textures/fast/red/textura cielo roja_.mov"/>
            </INPUT>
            <INPUT name="Fast-Blue" type="IMAGE" bpm="100">
                <ASSET name="ciudad distorsion 2" path="sets/textures/fast/blue/ciudad distorsión2 .mov"/>
                <ASSET name="ciudad_cielo_difuso_colores " path="sets/textures/fast/blue/ciudad_cielo_difuso_colores .mov"/>
                <ASSET name="ciudad invertida azul " path="sets/textures/fast/blue/ciudad invertida azul .mov"/>
            </INPUT>
            <INPUT name="Fast-Green" type="IMAGE" bpm="100">
                <ASSET name="conteiners colores " path="sets/textures/fast/green/conteiners colores .mov"/>
                <ASSET name="conteiners2.2" path="sets/textures/fast/green/conteiners2.2.mov"/>
                <ASSET name="vertical_ciudad " path="sets/textures/fast/green/vertical_ciudad .mov"/>
            </INPUT>
            <INPUT name="Fast-Red2" type="IMAGE" bpm="100">
                <ASSET name="edificios rojos  " path="sets/textures/fast/red/edificios rojos  .mov"/>
                <ASSET name="textura roja" path="sets/textures/fast/red/textura cielo roja_.mov"/>
            </INPUT>
            <INPUT name="Fast-Blue2" type="IMAGE" bpm="100">
                <ASSET name="ciudad distorsion 2" path="sets/textures/fast/blue/ciudad distorsión2 .mov"/>
                <ASSET name="ciudad_cielo_difuso_colores " path="sets/textures/fast/blue/ciudad_cielo_difuso_colores .mov"/>
                <ASSET name="ciudad invertida azul " path="sets/textures/fast/blue/ciudad invertida azul .mov"/>
            </INPUT>
            <INPUT name="Fast-Green2" type="IMAGE" bpm="100">
                <ASSET name="conteiners colores " path="sets/textures/fast/green/conteiners colores .mov"/>
                <ASSET name="conteiners2.2" path="sets/textures/fast/green/conteiners2.2.mov"/>
                <ASSET name="vertical_ciudad " path="sets/textures/fast/green/vertical_ciudad .mov"/>
            </INPUT>
            <!-- slow -->
            <INPUT name="Slow-Blue" type="IMAGE" bpm="100">
                <ASSET name="ciudad_azul " path="sets/textures/slow/blue/ciudad_azul .mov"/>
                <ASSET name="mar" path="sets/textures/slow/blue/mar.mov"/>
                <ASSET name="hojas" path="sets/textures/slow/blue/hojas.mov"/>
                <ASSET name="letras_invertidas_" path="sets/textures/slow/blue/letras_invertidas_.mov"/>
                <ASSET name="reflejo_texturas " path="sets/textures/slow/blue/reflejo_texturas .mov"/>
                <ASSET name="textura hojas cielo" path="sets/textures/slow/blue/textura hojas cielo.mov"/>
                <ASSET name="vertical" path="sets/textures/slow/blue/vertical_.mov"/>
            </INPUT>
            <INPUT name="Slow-Red" type="IMAGE" bpm="100">
                <ASSET name="ciudad roja " path="sets/textures/slow/red/ciudad roja .mov"/>
                <ASSET name="luz cielo " path="sets/textures/slow/red/luz cielo.mov"/>
                <ASSET name="textura hojas verde y rosado" path="sets/textures/slow/red/textura hojas verde y rosado.mov"/>
            </INPUT>
            <INPUT name="Medium-Blue" type="IMAGE" bpm="100">
                <ASSET name="ciudad contraluz violeta " path="sets/textures/medium/blue/ciudad contraluz violeta .mov"/>
                <ASSET name="letras_invertidas_2" path="sets/textures/medium/blue/letras_invertidas_2.mov"/>
            </INPUT>
            <INPUT name="Medium-Green" type="IMAGE" bpm="100">
                <ASSET name="edificio_distort_" path="sets/textures/medium/green/edificio_distort_.mov"/>
                <ASSET name="textura pasto giro normal 1" path="sets/textures/medium/green/textura pasto giro normal 1.mov"/>
            </INPUT>
            <INPUT name="Medium-Red" type="IMAGE" bpm="100">
                <ASSET name="ciudad_roja2" path="sets/textures/medium/red/ciudad_roja2.mov"/>
                <ASSET name="letras_invertidas_2" path="sets/textures/medium/red/espinas rojas_ cielo.mov"/>
            </INPUT>
            <INPUT name="Slow-Blue2" type="IMAGE" bpm="100">
                <ASSET name="ciudad_azul " path="sets/textures/slow/blue/ciudad_azul .mov"/>
                <ASSET name="mar" path="sets/textures/slow/blue/mar.mov"/>
                <ASSET name="hojas" path="sets/textures/slow/blue/hojas.mov"/>
                <ASSET name="letras_invertidas_" path="sets/textures/slow/blue/letras_invertidas_.mov"/>
                <ASSET name="reflejo_texturas " path="sets/textures/slow/blue/reflejo_texturas .mov"/>
                <ASSET name="textura hojas cielo" path="sets/textures/slow/blue/textura hojas cielo.mov"/>
                <ASSET name="vertical" path="sets/textures/slow/blue/vertical_.mov"/>
            </INPUT>
            <INPUT name="Slow-Red2" type="IMAGE" bpm="100">
                <ASSET name="ciudad roja " path="sets/textures/slow/red/ciudad roja .mov"/>
                <ASSET name="luz cielo " path="sets/textures/slow/red/luz cielo.mov"/>
                <ASSET name="textura hojas verde y rosado" path="sets/textures/slow/red/textura hojas verde y rosado.mov"/>
            </INPUT>
            <INPUT name="Medium-Blue2" type="IMAGE" bpm="100">
                <ASSET name="ciudad contraluz violeta " path="sets/textures/medium/blue/ciudad contraluz violeta .mov"/>
                <ASSET name="letras_invertidas_2" path="sets/textures/medium/blue/letras_invertidas_2.mov"/>
            </INPUT>
            <INPUT name="Medium-Green2" type="IMAGE" bpm="100">
                <ASSET name="edificio_distort_" path="sets/textures/medium/green/edificio_distort_.mov"/>
                <ASSET name="textura pasto giro normal 1" path="sets/textures/medium/green/textura pasto giro normal 1.mov"/>
            </INPUT>
            <INPUT name="Medium-Red2" type="IMAGE" bpm="100">
                <ASSET name="ciudad_roja2" path="sets/textures/medium/red/ciudad_roja2.mov"/>
                <ASSET name="letras_invertidas_2" path="sets/textures/medium/red/espinas rojas_ cielo.mov"/>
            </INPUT>
            <!--Particle and Cam -->
            <INPUT name="Particle" type="PARTICLE"/>
            <INPUT name="Cam" type="CAM"/>
            
        </INPUTS>
        <VISUAL_LAYERS>
            <VISUAL_LAYER name="Particle Visual" type="IMAGE_PROCESSOR" inputSource="Particle"/>

            

            <!-- Momo Pipeline  Visual_Layers-->
            <VISUAL_LAYER name="Momo Visual 0" type="IMAGE_PROCESSOR" inputSource="Momo Input Selector 0"/>
            <VISUAL_LAYER name="Momo Visual 1" type="IMAGE_PROCESSOR" inputSource="Momo Input Selector 1"/>
            <!-- Momo Pipeline  Visual_Layers END-->

            <!-- Fast Hard Pipeline  Visual_Layers-->
            <VISUAL_LAYER name="Fast Hard Visual" type="IMAGE_PROCESSOR" inputSource="Fast Hard Input Selector 0"
                isContrast="true" coBrightness="2.5"
                />
            <!-- Fast Hard Pipeline  Visual_Layers END-->

            <!-- Slow Pipeline  Visual_Layers-->
            <VISUAL_LAYER name="Slow Visual" type="IMAGE_PROCESSOR" inputSource="Slow Input Selector 0"
                isZoomBlur="true" 
                />
            <!-- Slow Pipeline  Visual_Layers END-->

            <!-- Psycho Pipeline  Visual_Layers-->
            <VISUAL_LAYER name="Psycho Visual 0" type="IMAGE_PROCESSOR" inputSource="Psycho Input Selector 0"
                isNoise="true"/>
            <VISUAL_LAYER name="Psycho Visual 1" type="IMAGE_PROCESSOR" inputSource="Psycho Input Selector 1"
                isNoise="true"/>
            <!-- Psycho Pipeline  Visual_Layers END-->

            <!-- IKEDA Pipeline  Visual_Layers-->
            <VISUAL_LAYER name="Ikeda Visual" type="IKEDA" inputSource="Ikeda Input Selector 0"/>
            <!-- IKEDA Pipeline  Visual_Layers END-->

            <!-- Particle Pipeline  Visual_Layers-->
            <VISUAL_LAYER name="Particle Visual" type="IMAGE_PROCESSOR" inputSource="Particle" isBloom="true" isZoomBlur="true" coContrast="1"
                />
            <!-- Particle Pipeline  Visual_Layers END-->

            
            <!-- GLITCH -->
            <VISUAL_LAYER name="Glitch Mix" type="GLITCH_1" inputSource="Glitch Input Selector"
                do_CONVERGENCE="false"
                do_GLOW="false"
                do_SHAKER="false"
                de_CUTSLIDER="false"
                do_TWIST="false"
                do_OUTLINE="false"
                do_NOISE="false"
                do_SLITSCAN="false"
                do_SWELL="false"
                do_INVERT="false"
                
                do_CR_HIGHCONTRAST="false"
                do_CR_BLUERAISE="false"
                do_CR_REDRAISE="false"
                do_CR_GREENRAISE="false"
                do_CR_BLUEINVERT="false"
                do_CR_REDINVERT="false"
                do_CR_GREENINVERT="false"/>

            <!-- PARTICLE MIX -->
            
        </VISUAL_LAYERS>

        <MIXERS>

            <!-- Momo Pipeline Mixers-->
            <MIXER name="Momo mix" type="SIMPLE_BLEND" selectorRight="1">
                <INPUT_SOURCE name="Momo Visual 0"/>
                <INPUT_SOURCE name="Momo Visual 1"/>
            </MIXER>
            <MIXER name="Momo Input Selector 0" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Black"/>
                <INPUT_SOURCE name="FARC"/>
                <INPUT_SOURCE name="Medium-Red"/>
                <INPUT_SOURCE name="Medium-Green"/>
                <INPUT_SOURCE name="Medium-Blue"/>
                <INPUT_SOURCE name="Particle"/>
                <INPUT_SOURCE name="Cam"/>
            </MIXER>
            <MIXER name="Momo Input Selector 1" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Black"/>
                <INPUT_SOURCE name="FARC"/>
                <INPUT_SOURCE name="imgSequences"/>
                <INPUT_SOURCE name="Nature"/>
                <INPUT_SOURCE name="City"/>
                <INPUT_SOURCE name="Medium-Red2"/>
                <INPUT_SOURCE name="Medium-Green2"/>
                <INPUT_SOURCE name="Medium-Blue2"/>
                <INPUT_SOURCE name="Particle"/>
                <INPUT_SOURCE name="Cam"/>
            </MIXER>
            <!-- Momo Pipeline Mixers END-->

            <!-- Fast Hard Mixers-->
            <MIXER name="Fast Hard Mix" type="SIMPLE_BLEND"   selectorRight="1" blendmode="17" opacity="255">
                <INPUT_SOURCE name="Fast Hard Visual"/>
                <INPUT_SOURCE name="Fast Hard Input Selector 1"/>
            </MIXER>
            <MIXER name="Fast Hard Input Selector 0" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Fast-Red"/>
                <INPUT_SOURCE name="Fast-Green"/>
                <INPUT_SOURCE name="Fast-Blue"/>
                <INPUT_SOURCE name="Black"/>
            </MIXER>
            <MIXER name="Fast Hard Input Selector 1" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Fast-Red2"/>
                <INPUT_SOURCE name="Fast-Green2"/>
                <INPUT_SOURCE name="Fast-Blue2"/>
                <INPUT_SOURCE name="Black"/>
            </MIXER>
            <!-- Fast Hard Mixers END-->

            <!-- Slow Mixers-->
            <MIXER name="Slow Mix" type="SIMPLE_BLEND"   selectorRight="1" opacity="100">
                <INPUT_SOURCE name="Slow Visual"/>
                <INPUT_SOURCE name="Slow Input Selector 1"/>
            </MIXER>
            <MIXER name="Slow Input Selector 0" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Slow-Red"/>
                <INPUT_SOURCE name="Slow-Blue"/>
                <INPUT_SOURCE name="Black"/>
            </MIXER>
            <MIXER name="Slow Input Selector 1" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Slow-Blue2"/>
                <INPUT_SOURCE name="Slow-Red2"/>
                <INPUT_SOURCE name="Black"/>
            </MIXER>
            <!-- Slow Mixers END-->

            <!-- Psycho Pipeline Mixers-->
            <MIXER name="Psycho Mix" type="SIMPLE_BLEND" selectorRight="1">
                <INPUT_SOURCE name="Psycho Visual 0"/>
                <INPUT_SOURCE name="Psycho Visual 1"/>
            </MIXER>
            <MIXER name="Psycho Input Selector 0" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Medium-Green"/>
                <INPUT_SOURCE name="Medium-Red"/>
                <INPUT_SOURCE name="Medium-Blue"/>
                <INPUT_SOURCE name="Black"/>
            </MIXER>
            <MIXER name="Psycho Input Selector 1" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Medium-Red2"/>
                <INPUT_SOURCE name="Medium-Green2"/>
                <INPUT_SOURCE name="Medium-Blue2"/>
                <INPUT_SOURCE name="Black"/>
            </MIXER>
            <!-- Psycho Pipeline Mixers END-->

            <!-- Ikeda Mixers-->
            <MIXER name="Ikeda Mix" type="SIMPLE_BLEND"   selectorRight="1" opacity="0">
                <INPUT_SOURCE name="Ikeda Visual"/>
                <INPUT_SOURCE name="Ikeda Input Selector 1"/>
            </MIXER>
            <MIXER name="Ikeda Input Selector 0" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Cam"/>
                <INPUT_SOURCE name="City"/>
                <INPUT_SOURCE name="Nature"/>
            </MIXER>
            <MIXER name="Ikeda Input Selector 1" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Black"/>
                <INPUT_SOURCE name="Fast-Red2"/>
                <INPUT_SOURCE name="Fast-Green2"/>
                <INPUT_SOURCE name="Fast-Blue2"/>
            </MIXER>
            <!-- Ikeda Mixers END-->

            <!-- Particle Mixers-->
            <MIXER name="Particle Mix" type="SIMPLE_BLEND"   selectorRight="1" blendmode="13" opacity="255">
                <INPUT_SOURCE name="Particle Visual"/>
                <INPUT_SOURCE name="Particle Input Selector"/>
            </MIXER>
            <MIXER name="Particle Input Selector" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Black"/>
                <INPUT_SOURCE name="imgSequences"/>
                <INPUT_SOURCE name="Slow-Blue2"/>
                <INPUT_SOURCE name="Nature"/>
                <INPUT_SOURCE name="City"/>
                
            </MIXER>
            <!-- Particle Mixers END-->

            <!-- Glitch -->
            <MIXER name="Glitch Input Selector" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Nature"/>
                <INPUT_SOURCE name="Cam"/>
                <INPUT_SOURCE name="Black"/>
            </MIXER>
            <!-- glitch END -->

            <!-- MIX for Columns -->
            <MIXER name="Columns" type="SIMPLE_BLEND" selectorRight="1">
                <INPUT_SOURCE name="selColumns1"/>
                <INPUT_SOURCE name="selColumns2"/>
            </MIXER>
            <!-- add input channels to all of the mixers -->
            <MIXER name="selColumns1" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Black"/>
                <INPUT_SOURCE name="Fast Hard Mix"/>
                <INPUT_SOURCE name="Particle Mix"/>
                <!--INPUT_SOURCE name="Momo mix"/-->
            </MIXER>
            <MIXER name="selColumns2" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Black"/>
                <INPUT_SOURCE name="Fast Hard Mix"/>
                <INPUT_SOURCE name="Particle Mix"/>
            </MIXER>
            
            <!-- MIX for Background -->
            <MIXER name="Background" type="SIMPLE_BLEND" selectorRight="1">
                <INPUT_SOURCE name="selBackground1"/>
                <INPUT_SOURCE name="selBackground2"/>
            </MIXER>
            <!-- add input channels to all of the mixers -->
            <MIXER name="selBackground1" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Black"/>
                <INPUT_SOURCE name="FARC"/>
                <INPUT_SOURCE name="Slow Mix"/>
                <INPUT_SOURCE name="Psycho Mix"/>
                <INPUT_SOURCE name="Ikeda Mix"/>
                <INPUT_SOURCE name="Glitch Mix"/>
                <INPUT_SOURCE name="Particle Mix"/>
            </MIXER>
            <MIXER name="selBackground2" type="MULTI_CHANNEL">
                <INPUT_SOURCE name="Black"/>
                <INPUT_SOURCE name="FARC"/>
                <INPUT_SOURCE name="Slow Mix"/>
                <INPUT_SOURCE name="Psycho Mix"/>
                <INPUT_SOURCE name="Ikeda Mix"/>
                <INPUT_SOURCE name="Glitch Mix"/>
                <INPUT_SOURCE name="Particle Mix"/>
            </MIXER>
        </MIXERS>

    </SETTINGS>

